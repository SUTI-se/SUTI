				<xs:complexType mixed="false">
					<xs:complexContent>
						<xs:extension base="addressType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="timesNode" type="timesType" minOccurs="0"/>
			<xs:element name="contents" type="contents" minOccurs="0"/>
			<xs:element name="nodeprocess" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="nodeprocess"/>
					</xs:complexContent>
