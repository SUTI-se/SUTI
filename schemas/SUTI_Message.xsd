								<xs:complexType>
									<xs:sequence>
										<xs:element name="distanceCount" type="xs:float"/>
										<xs:element name="distanceUnitPrice" type="xs:float"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="durationTotalPrice">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="durationCount" type="xs:float"/>
										<xs:element name="durationUnitPrice" type="xs:float"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
