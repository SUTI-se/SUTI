				<xs:complexType>
					<xs:sequence>
						<xs:element name="exceptionDate" type="date" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="additionalDates" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="additionalDate" type="date" minOccurs="0" maxOccurs="unbounded"/>
